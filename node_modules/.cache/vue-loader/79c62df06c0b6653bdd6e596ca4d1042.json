ction() {\n            if (this.tempRate != 0) {\n                var sum = this.card.rate * this.card.rateNumber;\n                this.card.rateNumber++;\n                this.card.rate = (sum + this.tempRate) / this.rateNumber;\n\n                axios.post(BASE_URL + \"add-rate\", {\n                    newRate: this.tempRate\n                })\n                .then((response) => {\n                    this.card.rate = response.data.rate\n                });\n            } \n        }\n    }\n\n\n\n\n}\n</script>\n\n<style>\n\n</style>"]}]}